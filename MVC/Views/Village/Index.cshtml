@model IEnumerable<MVC.Models.Village>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CityName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VillageId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VillageName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CityName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VillageId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VillageName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.VillageId }) |
            @Html.ActionLink("Details", "Details", new { id=item.VillageId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.VillageId })
        </td>
    </tr>
}

</table>
